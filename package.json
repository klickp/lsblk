{
  "name": "ordering-system",
  "version": "1.0.0",
  "description": "Full-stack ordering system with React, Node.js, and TiDB",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
