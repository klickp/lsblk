    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build"
  },
  "keywords": ["ordering-system", "restaurant", "fullstack"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
